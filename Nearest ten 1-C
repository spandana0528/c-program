#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() 
{
    int num, remainder, nearestMultiple;
    scanf("%d", &num);
    remainder = num % 10;
    if (remainder < 5)
    {
        nearestMultiple = num - remainder;
    } else {
        nearestMultiple = num + (10 - remainder);
    }
    printf("%d\n", nearestMultiple);

    return 0;
}
