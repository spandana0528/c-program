#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
    int n = b * 75 + c * 30;
    if (n <= a)
    { 
        printf("Boat is stable\n");
    } 
    else
    {
        printf("Boat will drown\n");
    }


       
    return 0;
}
