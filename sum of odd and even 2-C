#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
int a[5],i,n,esum,osum;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0,esum=0,osum=0;i<n;i++){
        if(a[i]%2==0){
            esum=esum+a[i];
            }
            else{
                osum=osum+a[i];
                }
            }
            printf("%d\n",osum);
            printf("%d",esum);
            
    return 0;
}
